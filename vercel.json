{
  "version": 2,
  "builds": [
    { "src": "api/api.py", "use": "@vercel/python" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/upscale", "dest": "api/api.py" },
    { "src": "/api/removebg", "dest": "api/api.py" },
    { "src": "/api/meme", "dest": "api/api.py" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}

